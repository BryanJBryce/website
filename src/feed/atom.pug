include _entry

- articles = mvb.articles
- feedUrl = h.siteUrl("/atom.xml")

doctype xml
feed(xmlns="http://www.w3.org/2005/Atom" xml:lang="en" xml:base="https://dennisreimann.de")
  id= feedUrl
  title Dennis Reimann
  subtitle Software development: Elixir, Elm, JavaScript, CSS, and HTML
  updated= h.articles.feedDate(mvb.articles)
  link(href= h.siteUrl("/articles.html"))
  link(rel="self" href=feedUrl)
  author
    name Dennis Reimann
    email mail@dennisreimann.de
  for a in articles
    +entry(a)
